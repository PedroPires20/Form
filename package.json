    "start": "yarn server start"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/ramda": "^0.27.64",
    "@types/uuid": "^8.3.4",
    "lerna": "^4.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "jest": "^27.4.7",
    "pm2": "^5.1.2",
    "ramda": "^0.27.2",
    "uuid": "^8.3.2"
  }
}
